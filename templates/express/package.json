    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "husky": "^9.1.1",
    "nodemon": "^2.0.22",
    "prettier": "^3.3.3",
    "prisma": "^4.12.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.5.3"
